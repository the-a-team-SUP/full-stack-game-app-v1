{
  "name": "full-stack-game-app-v1",
  "version": "1.0.0",
  "description": "this game is a full stack app that made of Epress, React, Redux, PostGres SQL and Sequelize as well as Migration",
  "main": "index.js",
  "scripts": {
    "server": "nodemon --exec babel-node ./server/index.js",
    "start": "cross-env NODE_ENV=production npx sequelize-cli db:migrate && cross-env NODE_ENV=production babel-node ./server/index.js",
    "createtables:dev": "npx sequelize db:migrate",
    "seed:dev": "npx sequelize db:seed:all",
    "droptables:dev": "npx sequelize db:migrate:undo:all",
    "kill": "Taskkill /IM node.exe /F",
    "lint": "eslint ./server",
    "client-install": "cd client && npm install",
    "client-start": "npm run start -- prefix client",
    "client-build": "npm run build -- prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client-start\"",
    "heroku-postbuild": "npm run client-install && npm run client-build",
    "production": "concurrently \"npm run start\" \"npm run client-build\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-a-team-SUP/full-stack-game-app-v1.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/the-a-team-SUP/full-stack-game-app-v1/issues"
  },
  "homepage": "https://github.com/the-a-team-SUP/full-stack-game-app-v1#readme",
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "bcrypt": "^4.0.0",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "path": "^0.12.7",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/node": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/register": "^7.8.6",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^5.5.1"
  }
}
