{
  "name": "full-stack-game-app-v1",
  "version": "1.0.0",
  "description": "this game is a full stack app that made of Epress, React, Redux, PostGres SQL and Sequelize as well as Migration",
  "main": "index.js",
  "scripts": {
    "server": "nodemon --exec babel-node ./server/index.js",
    "start": "cross-env NODE_ENV=production npx sequelize-cli db:migrate && cross-env NODE_ENV=production babel-node ./server/index.js",
    "createtables:dev": "cross-env NODE_ENV=development npx sequelize db:migrate",
    "seed:dev": "cross-env NODE_ENV=development npx sequelize db:seed:all",
    "droptables:dev": "cross-env NODE_ENV=development npx sequelize db:migrate:undo:all",
    "createtables:test": "cross-env NODE_ENV=test npx sequelize db:migrate",
    "seed:test": "cross-env NODE_ENV=test npx sequelize db:seed:all",
    "droptables:test": "cross-env NODE_ENV=test npx sequelize db:migrate:undo:all",
    "kill": "Taskkill /IM node.exe /F",
    "lint": "eslint ./server",
    "client-install": "cd client && npm install",
    "client-start": "cd client && npm start",
    "client-build": "cd client && npm run build",
    "dev": "concurrently \"npm run server\" \"npm run client-start\"",
    "heroku-postbuild": "npm run client-install && npm run client-build",
    "production": "concurrently \"npm run start\" \"npm run client-build\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-a-team-SUP/full-stack-game-app-v1.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/the-a-team-SUP/full-stack-game-app-v1/issues"
  },
  "homepage": "https://github.com/the-a-team-SUP/full-stack-game-app-v1#readme",
  "dependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "babel-core": "^6.26.3",
    "babel-install": "2.1.0",
    "babel-loader": "^8.0.6",
    "babel-node": "0.0.1-security",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^3.0.6",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "path": "^0.12.7",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.5",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^5.5.1"
  }
}
